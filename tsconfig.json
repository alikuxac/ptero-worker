{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es2021",
    "lib": ["esnext", "es2021"],
    "alwaysStrict": true,
    "strict": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "types": [
      "@cloudflare/workers-types",
    ]
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist", "test"]
}
